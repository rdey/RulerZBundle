{
    "name": "redeye/rulerz-bundle",
    "type": "symfony-bundle",
    "license": "MIT",
    "description": "Symfony2 Bundle for RulerZ",
    "keywords": ["specification", "doctrine", "ruler", "rulerz"],
    "autoload": {
        "psr-4": { "KPhoen\\RulerZBundle\\": "" },
        "exclude-from-classmap": [
            "/Tests/"
        ]
    },
    "require": {
        "php": ">=7.1",
        "redeye/rulerz-bridge": "dev-sf5",
        "redeye/rulerz": "dev-sf5",
        "symfony/config": "^3.0|^4.0|^5.0",
        "symfony/framework-bundle": "^3.0|^4.0|^5.0"
    },
    "require-dev": {
        "liip/rmt": "^1.2",
        "matthiasnoback/symfony-dependency-injection-test": "^3.0",
        "mikey179/vfsstream": "^1.6",
        "phpunit/phpunit": "^7.1",
        "symfony/phpunit-bridge": "^3.0|^4.0|^5.0"
    }
}
